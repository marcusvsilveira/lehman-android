<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".GameSettingsActivity" >
    
    <TextView
         android:id="@+id/TextView04"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBaseline="@+id/TextView02"
         android:layout_alignBottom="@+id/TextView02"
         android:layout_alignRight="@+id/dog"
         android:text="max_speed" />
    
    <TextView
         android:id="@+id/TextView02"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/dog"
         android:layout_below="@+id/dog"
         android:text="min_speed" />
    
    <SeekBar
         android:id="@+id/fox"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/TextView02"
         android:layout_below="@+id/TextView06"
         android:max="10" />
    
    <TextView
         android:id="@+id/TextView03"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/fox"
         android:layout_below="@+id/fox"
         android:text="max_speed" />

     
    <TextView
         android:id="@+id/TextView01"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/fox"
         android:layout_alignTop="@+id/TextView03"
         android:text="min_speed" />
    
    <SeekBar
         android:id="@+id/sheep"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/TextView03"
         android:layout_below="@+id/TextView01"
         android:layout_marginTop="76dp"
         android:max="10" />
    
    
    <TextView
         android:id="@+id/sheep_text2"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBaseline="@+id/sheep_text"
         android:layout_alignBottom="@+id/sheep_text"
         android:layout_alignRight="@+id/sheep"
         android:text="max_speed" />

     <TextView
         android:id="@+id/sheep_text"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/sheep"
         android:layout_below="@+id/sheep"
         android:text="min_speed" />

     <Button
         android:id="@+id/backButton"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentBottom="true"
         android:layout_centerHorizontal="true"
         android:text="@string/back" />

     <TextView
         android:id="@+id/TextView06"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/TextView05"
         android:layout_below="@+id/TextView02"
         android:layout_marginTop="48dp"
         android:text="Fox"
         android:textColor="#E40045" />
     
     <TextView
         android:id="@+id/textV06"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/fox"
         android:layout_alignTop="@+id/fox"
         android:singleLine="true"
         android:textColor="#E40045" />

     <TextView
         android:id="@+id/TextView07"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBottom="@+id/sheep"
         android:layout_alignLeft="@+id/sheep"
         android:layout_marginBottom="32dp"
         android:text="Sheep"
         android:textColor="#E40045" />
     
     <TextView
         android:id="@+id/textV07"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/sheep"
         android:layout_alignTop="@+id/sheep"
         android:singleLine="true"
         android:textColor="#E40045">
     </TextView>
        
    
     <SeekBar
         android:id="@+id/dog"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_alignParentLeft="true"
         android:layout_below="@+id/TextView05"
         android:max="10"/>

     <TextView
         android:id="@+id/TextView05"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/dog"
         android:layout_alignParentTop="true"
         android:layout_marginTop="22dp"
         android:text="Dog"
         android:textColor="#E40045" />
     
     <TextView
         android:id="@+id/textV05"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/dog"
         android:layout_alignTop="@+id/dog"
         android:singleLine="true"
         android:textColor="#E40045" />
     
</RelativeLayout>
